<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent" android:layout_height="wrap_content">
    <RelativeLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <ImageView android:id="@+id/gps_result" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="50dp" android:src="@drawable/gps_support" android:layout_centerHorizontal="true" />
        <LinearLayout android:orientation="vertical" android:id="@+id/gps_test_textview" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="20dp" android:layout_below="@+id/gps_result">
            <TextView android:textSize="18sp" android:textColor="#f47424" android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:id="@+id/result_test" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/result_ok" />
            <TextView android:textSize="15sp" android:textColor="#686868" android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:id="@+id/mob_model" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="9dp" android:text="@string/mobe_model" />
            <TextView android:textSize="15sp" android:textColor="#686868" android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:id="@+id/andr_version" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:text="@string/android_version" />
            <TextView android:textSize="12sp" android:textColor="#a7a7a7" android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end" android:id="@+id/error_reason" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="50dp" android:layout_marginTop="20dp" android:layout_marginRight="50dp" android:text="@string/error_all" />
        </LinearLayout>
        <Button android:textSize="15sp" android:textColor="#ffffff" android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:id="@+id/use_begin" android:background="@drawable/gps_running_btn_know" android:layout_width="UNKNOWN_DATA_0x9901" android:layout_height="UNKNOWN_DATA_0x2101" android:layout_marginTop="25dp" android:text="@string/use_begin" android:layout_below="@+id/gps_test_textview" android:layout_centerHorizontal="true" />
    </RelativeLayout>
</ScrollView>
